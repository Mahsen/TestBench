//_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_
TAGBENCH::struct_Parm* TAGBENCH::Detach(U8* Data)
{
	static TAGBENCH::struct_Parm Parm;
	char *pch;
	
	Parm.ResetCommand();
	Parm.ResetData();
	
	pch = strtok((char*)Data, "<>:");
	if(pch)
	{
		if(strcmp((char*)pch, "TEST_BENCH") == NULL)
		{
			pch = strtok(NULL, "<>:");			
			if(pch)
			{
				strcpy((char*)Parm.Command, pch);
				pch = strtok(NULL, "<>");				
				if(pch)
				{
					if(strcmp((char*)pch, "/TEST_BENCH") == NULL)
					{
						return &Parm;
					}
					else
					{
						strcpy((char*)Parm.Data, pch);
						pch = strtok(NULL, "<>:");
						if(pch)
						{
							if(strcmp((char*)pch, "/TEST_BENCH") == NULL)
							{
								return &Parm;
							}
						}
					}
				}
			}			
		}
	}

	Parm.ResetCommand();
	Parm.ResetData();
	
	return &Parm;
}
//_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_
U8* TAGBENCH::Attach(TAGBENCH::struct_Parm* Parm)
{
	static TAGBENCH::struct_Parm Buffer;
	
	Buffer.ResetCommand();
	Buffer.ResetData();
	
	sprintf((char*)Buffer.Data, "<TEST_BENCH:%s>%s</TEST_BENCH>\r\n", Parm->GetCommand(), Parm->GetData());
	
	return Buffer.GetData();
}
